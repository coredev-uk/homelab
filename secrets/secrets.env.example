# Example secrets file - copy to secrets.env and fill in real values

# Pihole
PIHOLE_WEBPASSWORD=your_admin_password_here

# Frigate
FRIGATE_MQTT_PASSWORD=your_mqtt_password_here

# VPN (WireGuard)
# Private key from your VPN provider - will be automatically base64 encoded by kubectl
WIREGUARD_PRIVATE_KEY=your_wireguard_private_key_here

# Cloudflare
CLOUDFLARE_API_TOKEN=your_cloudflare_token_here

# Notifiarr
NOTIFIARR_API_KEY=your_notifiarr_api_key_here
NOTIFIARR_SONARR_API_KEY=your_sonarr_api_key_here
NOTIFIARR_RADARR_API_KEY=your_radarr_api_key_here
NOTIFIARR_PROWLARR_API_KEY=your_prowlarr_api_key_here
NOTIFIARR_BAZARR_API_KEY=your_bazarr_api_key_here
NOTIFIARR_QBIT_USER=your_qbittorrent_username_here
NOTIFIARR_QBIT_PASS=your_qbittorrent_password_here
NOTIFIARR_SABNZBD_API_KEY=your_sabnzbd_api_key_here

# Glance (see https://open-meteo.com/en/docs/geocoding-api)
GLANCE_WEATHER_LOCATION="London, United Kingdom"
