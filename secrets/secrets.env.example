# Example secrets file - copy to secrets.env and fill in real values

# Pihole
PIHOLE_WEBPASSWORD=your_admin_password_here

# Frigate
FRIGATE_MQTT_PASSWORD=your_mqtt_password_here
FRIGATE_RTSP_USER=your_rtsp_username_here
FRIGATE_RTSP_PASSWORD=your_rtsp_password_here

# VPN - Separate keys for SABnzbd and qFlood
SAB_VPN_PRIVATE_KEY=your_sabnzbd_wireguard_private_key_here
QBIT_VPN_PRIVATE_KEY=your_qbittorrent_wireguard_private_key_here
PROTON_VPN_EMAIL=your_proton_vpn_email_here
PROTON_VPN_PASSWORD=your_proton_vpn_password_here

# Cloudflare
CLOUDFLARE_API_TOKEN=your_cloudflare_token_here

# Cloudflare Tunnel Token
CLOUDFLARE_TUNNEL_TOKEN=your_tunnel_token_here

# Notifiarr
NOTIFIARR_API_KEY=your_notifiarr_api_key_here

# Glance (see https://open-meteo.com/en/docs/geocoding-api)
GLANCE_WEATHER_LOCATION="Your City, Your Country"

# Sonarr and Radarr API keys (for Glance)
SONARR_API_KEY=your_sonarr_api_key_here
RADARR_API_KEY=your_radarr_api_key_here

# Token for Repository Fetching
GITHUB_TOKEN=your_github_token_here

# Authelia & LLDAP
LLDAP_ADMIN_PASSWORD=your_lldap_admin_password_here
LLDAP_JWT_SECRET=generate_with_openssl_rand_base64_32
AUTHELIA_STORAGE_ENCRYPTION_KEY=generate_with_openssl_rand_hex_32
AUTHELIA_JWT_SECRET=generate_with_openssl_rand_hex_32
AUTHELIA_SESSION_SECRET=generate_with_openssl_rand_hex_32
AUTHELIA_OIDC_HMAC_SECRET=generate_with_openssl_rand_hex_32
AUTHELIA_OIDC_PRIVATE_KEY=generate_rsa_key_see_readme
AUTHELIA_GRAFANA_CLIENT_SECRET=generate_with_openssl_rand_hex_32
AUTHELIA_ARGOCD_CLIENT_SECRET=generate_with_openssl_rand_hex_32
